    "body-parser": "^1.15.2",
    "ejs": "^2.4.2",
    "expect": "^1.20.2",
    "express": "^4.14.0",
    "express-jwt": "^3.4.0",
    "github": "0.2.4",
    "gitlab": "3.4.2",
